<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tuljain.requests">

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="19" />

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/images"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".FrontPage">
            <action android:name="com.example.tuljain.requests.FrontPage" />
            <category android:name="android.intent.category.DEFAULT" />
        </activity>

        <activity android:name=".Register">
            <action android:name="com.example.tuljain.requests.Register" />
            <category android:name="android.intent.category.DEFAULT" />
        </activity>
    </application>

</manifest>
